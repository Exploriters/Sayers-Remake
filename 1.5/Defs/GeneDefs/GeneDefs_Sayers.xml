    <!-- HEAD ALBINISM -->
    <GeneCategoryDef>
        <defName>SayersAlbinism_Head</defName>
        <ignoreIllegalLabelCharacterConfigError>true</ignoreIllegalLabelCharacterConfigError>
        <label>头部白化/深化[赛尔斯]</label>
        <displayPriorityInXenotype>-1000</displayPriorityInXenotype>
    </GeneCategoryDef>

    <GeneDef Name="SayersAlbinismBase_Head" Abstract="True">
        <biostatCpx>0</biostatCpx>
        <exclusionTags>
            <li>Sayers_Albinism_Head</li>
        </exclusionTags>
        <displayCategory>SayersAlbinism_Head</displayCategory>
        <randomChosen>true</randomChosen>
        <iconColor>(0.75, 0.75, 0.75)</iconColor>
    </GeneDef>

    <GeneDef ParentName="SayersAlbinismBase_Head">
        <defName>Sayers_Albinism_Head_None</defName>
